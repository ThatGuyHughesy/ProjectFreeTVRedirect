{
  "name": "ProjectFreeTV Redirect",
  "version": "0.2",
  "description": "No More 10 Second Wait",
  "background": {
    "page": "home.html"
  },
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  },
  "content_scripts": [{
    "matches": ["http://www.free-tv-video-online.info/*"],
    "js": ["content.js"]
  }],
  "browser_action": {
    "default_icon": "icon.png"
  },
  "manifest_version": 2,
  "permissions": ["activeTab"]
}
