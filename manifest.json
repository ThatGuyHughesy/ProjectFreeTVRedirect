{
  "name": "Redirection",
  "version": "0.4.1",
  "description": "No More Waiting Around For Videos",
  "background": {
    "scripts": ["scripts/services/background.js", "scripts/services/redirect.js"]
  },
  "icons": {
    "16": "images/16.png",
    "48": "images/48.png",
    "128": "images/128.png"
  },
  "content_scripts": [{
    "matches": ["http://*/*"],
    "js": ["scripts/jquery/jquery-2.1.3.min.js", "scripts/controllers/check.js"]
  }],
  "browser_action": {
    "default_icon": "images/logo.png",
    "default_popup": "home.html"
  },
  "manifest_version": 2,
  "permissions": [
    "activeTab"
  ]
}